<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/c2018450/ontologies/2024/6/TwitterUser/"
         xml:base="http://www.semanticweb.org/c2018450/ontologies/2024/6/TwitterUser/"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xml="http://www.w3.org/XML/1998/namespace"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:TwitterUser="http://www.semanticweb.org/c2018450/ontologies/2024/6/TwitterUser#">

    <!-- Ontology Declaration -->
    <owl:Ontology rdf:about="http://www.semanticweb.org/c2018450/ontologies/2024/6/TwitterUser"/>

    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="#hasAge">
        <rdfs:domain rdf:resource="#UserAccount"/>
        <rdfs:range rdf:resource="#Age"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#likes">
        <rdfs:domain rdf:resource="#Alice"/>
        <rdfs:range rdf:resource="#Tom"/>
    </owl:ObjectProperty>

    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="#age">
        <rdfs:domain rdf:resource="#UserAccount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>

    <owl:DatatypeProperty rdf:about="#name">
        <rdfs:domain rdf:resource="#UserAccount"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- Classes -->
    <owl:Class rdf:about="#UserAccount"/>

    <owl:Class rdf:about="#Age"/>

    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="#Age25">
        <rdf:type rdf:resource="#Age"/>
        <TwitterUser:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</TwitterUser:age>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Age30">
        <rdf:type rdf:resource="#Age"/>
        <TwitterUser:age rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">30</TwitterUser:age>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Alice">
        <rdf:type rdf:resource="#UserAccount"/>
        <TwitterUser:name>Alice</TwitterUser:name>
        <TwitterUser:hasAge rdf:resource="#Age25"/>
        <TwitterUser:likes rdf:resource="#Tom"/>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="#Tom">
        <rdf:type rdf:resource="#UserAccount"/>
        <TwitterUser:name>Tom</TwitterUser:name>
        <TwitterUser:hasAge rdf:resource="#Age30"/>
    </owl:NamedIndividual>

</rdf:RDF>
